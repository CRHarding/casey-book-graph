type User {
    id: Long!
    username: String
    email: String
    firstName: String
    lastName: String
    aboutMe: String
    password: String
}

type Mutation {
    createUser(input: InputCreateOneUser): User!
}

input InputCreateOneUser {
    username: String!
    firstName: String!
    lastName: String!
    email: String!
    aboutMe: String
    password: String!
}

type Query {
    findAllUsers: [User]
}

schema {
    query: Query
    mutation: Mutation
}